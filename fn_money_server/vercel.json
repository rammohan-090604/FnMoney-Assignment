{
    "version": 2,
    "builds": [
      {
        "src": "fn_money_server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "fn_money/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "fn_money/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "fn_money_server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  